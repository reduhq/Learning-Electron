{
  "name": "learningelectron",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "Rey Halsall",
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run next:dev\" \"npm run electron:dev\"",
    "next:dev": "cd renderer && npm run dev",
    "electron:dev": "wait-on http://localhost:3000 && electron electron/main.js",
    "build": "cd renderer && npm run build && npm run export"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
